#version 330 core

// Input data
layout(location = 0) in vec3 vertexPosition;	// Vertex position in object, from Main

// Output data
out vec3 TexCoords;

// Constants for object
uniform mat4 P;
uniform mat4 V;

void main()
{		
	gl_Position =  (P * V * vec4(vertexPosition, 1.0f)).xyww;
	TexCoords = vertexPosition;
}

